                    .col-md-4
                        h1.days-counter
                            script.
                                var startd = Date.now();
                                var endd = Date.parse('Sun, 08 Oct 2017 16:00:00 PDT');
                                var diff = new Date(endd - startd);
                                var days = diff / 1000 / 60 / 60 / 24;
                                document.write(Math.max(Math.floor(days), 0));
